{
  "version": 2,
  "name": "arena-toronto",
  "scope": "4-us-and-4-others",
  "builds": [
    { "src": "api/index.go", "use": "@now/go" },
    { "src": "www/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api", "dest": "api/index.go" },
    { "src": "/ta", "dest": "www/ta.html" },
    { "src": "/ac", "dest": "www/ac.html" },
    { "src": "/static/(.*)", "dest": "www/static/$1" },
    { "src": "/(.*)", "dest": "www/index.html" }
  ],
  "github": {
    "enabled": false,
    "silent": true
  }
}
